<h1>Calculate the plan costs!</h1>
<div class="index-container">
  <ul>
    <% @plans.each do |plan| %>
      <li>
        <div>Plan name: <%= plan.name %></div>
        <div>Your out of pocket cost: <%= plan.calculate_price([25, 30, 23, 45], [94150, 94606, 92116, 94610]) %></div>
      </li>
    <% end %>
  </ul>
</div>
